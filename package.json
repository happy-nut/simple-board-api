{
  "name": "simple-board-api-server",
  "version": "0.0.1",
  "description": "Simple board api server for class101 homework",
  "author": "happy-nut <happynut.dev@gmail.com>",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build -p ./tsconfig.build.json",
    "prestart": "rimraf dist",
    "start": "nest start",
    "lint": "eslint \"{src,test}/**/*.{ts,(i-)?spec.ts,js}\" --fix --color",
    "test": "jest --runInBand",
    "test:watch": "yarn test --watch",
    "test:cov": "yarn test --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "dependencies": {
    "@nestjs/cli": "^7.5.4",
    "@nestjs/common": "~7.5.1",
    "@nestjs/core": "~7.5.1",
    "@nestjs/platform-express": "~7.5.1",
    "@nestjs/typeorm": "~7.1.5",
    "config": "~3.3.3",
    "lodash": "^4.17.20",
    "mysql": "^2.18.1",
    "nanoid": "^3.1.20",
    "reflect-metadata": "^0.1.13",
    "ts-custom-error": "~3.2.0",
    "typeorm": "^0.2.29"
  },
  "devDependencies": {
    "@nestjs/swagger": "~4.7.5",
    "@nestjs/testing": "~7.5.1",
    "@types/config": "~0.0.38",
    "@types/express": "~4.17.8",
    "@types/jest": "~26.0.15",
    "@types/lodash": "^4.14.167",
    "@types/node": "~14.14.6",
    "@types/supertest": "~2.0.10",
    "@typescript-eslint/eslint-plugin": "~4.6.1",
    "@typescript-eslint/parser": "~4.6.1",
    "eslint": "~7.12.1",
    "jest": "~26.6.3",
    "jest-extended": "~0.11.5",
    "jest-mock-extended": "~1.0.10",
    "rimraf": "~3.0.2",
    "supertest": "~6.0.1",
    "swagger-ui-express": "~4.1.5",
    "ts-jest": "~26.4.3",
    "ts-loader": "~8.0.8",
    "ts-node": "~9.0.0",
    "typescript": "~4.0.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.(i-)?spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "setupFilesAfterEnv": [
      "./test/setUpTest.ts"
    ],
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
